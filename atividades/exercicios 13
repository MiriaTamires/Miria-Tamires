c-let i= 2;
let max= 0;
console.log("tabuada do" + i);
while(max<=10){
  console.log(i+"x"+max+":"+i*max);
  max++;
}
-------------------------------------------------------------------------------------------------------------------------------------------------
let i= 2;
let max= 0;
console.log("tabuada do" + i);
do{
  console.log(i+"x"+max+":"+i*max);
  max++;
}while(max<=10);
